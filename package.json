    "re2": "^1.17.7",
    "remark-cli": "^11.0.0",
    "remark-preset-github": "^4.0.4",
    "rimraf": "^3.0.2",
    "tinyify": "^3.1.0",
    "xo": "^0.50.0"
  },
  "engines": {
    "node": ">=14"
  },
